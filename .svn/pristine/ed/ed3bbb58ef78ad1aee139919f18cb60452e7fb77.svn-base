	<script type="text/javascript">
		$(function() {
			$("body").Huitab({
				tabBar: ".navbar-wrapper .navbar-levelone",
				tabCon: ".Hui-aside .menu_dropdown",
				className: "current",
				index: 0,
			});

		});
		// 退出
		function logout() {
			var datas = {}
			postAjax("sysuser/logout", datas, function(datas) {
				// if (datas.code == 0||datas.code == 93||) {
				window.location.href = "./login.html"
				//	}
			}, '', true);
		}
		//修改密碼
		function myselfinfo() {
			$("#modal-demo").modal("show")
		}
		$("#submitNewpwd").unbind('click').click(function(event) {
			var oldPwd = $("#oldPwd").val();
			var newPwd = $("#newPwd").val();
			var regex = new RegExp(/^(?![^a-zA-Z]+$)(?!\D+$)/);
			if (!oldPwd && !newPwd) {
				$.Huimodalalert('密码不能为空！', 1500)
			} else if (!oldPwd) {
				$.Huimodalalert('旧密码不能为空！', 1500)
			} else if (!newPwd) {
				$.Huimodalalert('新密码不能为空！', 1500)
			}

			if (newPwd && oldPwd) {
				if (!regex.test(newPwd) && !regex.test(oldPwd)) {
					$.Huimodalalert('密码格式不正确！', 1500)
				} else if (!regex.test(newPwd) && (newPwd.length < 6 || newPwd.length > 20)) {
					$.Huimodalalert('新密码格式不正确！', 1500)
				} else if (!regex.test(oldPwd) && (oldPwd.length < 6 || oldPwd.length > 20)) {
					$.Huimodalalert('旧密码格式不正确！', 1500)
				}
				if (regex.test(newPwd) && regex.test(oldPwd) && oldPwd.length >= 6 && oldPwd.length <= 20 && newPwd.length >= 6 && newPwd.length <= 20) {
					var data = {
						op: oldPwd,
						np: newPwd
					}
					postAjax("sysuser/update/password", data, function(msg) {
						if (msg.code == 0) {
							$.Huimodalalert('密码更改成功！', 1500)
							$("#modal-demo").modal("hide");
							var time = setTimeout(function() {
								window.location = "./login.html";
							}, 800)

						}
						if (msg.code == 91) {
							$.Huimodalalert("旧密码错误！", 1500)
						}
						// if()
					})
				}

			}
		});
	</script>
